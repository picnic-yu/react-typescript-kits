{
  "transform": {
    "^.+\\.jsx?$": "babel-jest"
  },
  "testRegex": "a.test.js",
  "moduleFileExtensions": [
    "js",
    "jsx"
  ],
  "browser": true,
  "collectCoverage": true,
  "collectCoverageFrom" : ["src/**/*.{js,jsx}", "!**/node_modules/**"],
  "coverageDirectory": "coverage",
  "moduleDirectories": ["node_modules", "./src"]
}
